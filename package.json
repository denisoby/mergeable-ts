{
  "name": "mergeable-ts",
  "version": "0.0.1",
  "description": "Library providing simple way to make changes into typescripts, much simplier than working with AST",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "watch": "tsc -w -p .",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "author": "Denis Obydennykh",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "ts-morph": "^2.3.0"
  }
}
